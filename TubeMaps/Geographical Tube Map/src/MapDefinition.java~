public class MapDefinition
{
	public MapDefinition()
	{
		description = "All Tube Map";
		fileBase = "/all_tube_";
		xTiles = 21;
		yTiles = 16;
		tileWidth = 100;
		tileHeight = 100;
		totalWidth = 2023;
		totalHeight = 1556;
		xCentre = 950-125;
		yCentre = 760-125;
	}
	public int xTiles;
	public int yTiles;
	public int tileWidth;
	public int tileHeight;
	public int totalWidth;
	public int totalHeight;
	public int xCentre;
	public int yCentre;
	public String description;
	public String fileBase;
}